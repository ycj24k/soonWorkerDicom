{
  "name": "soonview2",
  "version": "0.0.1",
  "author": "24kycj <1637269896@qq.com>",
  "description": "Cardsoon SoonWorkerDicom",
  "homepage": "https://www.cardsoon.com/index.php/cn",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:linux": "node .electron-vue/build.js && electron-builder --linux",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "electron:linux": "vue-cli-service electron:build -l --x64",
    "electron:arm": "vue-cli-service electron:build -l --arm64",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "npm run lint:fix"
  },
  "build": {
    "productName": "SoonView",
    "appId": "com.cardsoon.SoonView",
    "extraFiles": [

      {
        "from": "DICOM",
        "to": "./DICOM"
      }
    ],
    "linux": {
          "maintainer": "Jerry Xu <jerry79.xu@gmail.com>",
          "category": "Utility",
          "target": [
            {
              "target": "AppImage",
              "arch": ["x64", "arm64"]
            },
            {
              "target": "deb",
              "arch": ["x64", "arm64"]
            },
            {
              "target": "rpm",
              "arch": ["x64", "arm64"]
            }
          ],
          "icon": "public/images/favicon.svg"
        },
    "copyright": "Copyright@2023 cardsoon.com",
    "directories": {
      "output": "build"
    }
  },
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "archiver": "^7.0.1",
    "archiver-zip-encrypted": "^2.0.0",
    "axios": "^0.18.0",
    "core-js": "3",
    "cornerstone-core": "^2.6.1",
    "cornerstone-math": "^0.1.10",
    "cornerstone-tools": "^6.0.10",
    "cornerstone-wado-image-loader": "^4.13.2",
    "dayjs": "^1.11.13",
    "dcmjs": "^0.43.1",
    "dicom-parser": "^1.8.21",
    "element-ui": "^2.15.14",
    "fake-progress": "^1.0.4",
    "hammerjs": "^2.0.8",
    "js-cookie": "^3.0.5",
    "localforage": "^1.10.0",
    "multispinner": "^0.2.1",
    "nprogress": "^0.2.0",
    "polyfill-object.fromentries": "^1.0.1",
    "vue": "^2.5.16",
    "vue-electron": "^1.0.6",
    "vue-i18n": "7.8.1",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-electron": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.7",
    "@babel/eslint-parser": "^7.28.4",
    "@babel/plugin-transform-runtime": "7.23.7",
    "@babel/preset-env": "7.23.7",
    "@babel/register": "7.23.7",
    "ajv": "^6.5.0",
    "autoprefixer": "^8.0.0",
    "babel-loader": "8.4.1",
    "babel-minify-webpack-plugin": "^0.3.1",
    "cfonts": "^2.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "22.0.0",
    "electron-builder": "23.3.3",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-rebuild": "^3.2.9",
    "eslint": "^4.19.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "listr": "^0.14.3",
    "mini-css-extract-plugin": "0.4.0",
    "node-loader": "^0.6.0",
    "postcss": "7.0.0",
    "postcss-loader": "^4.0.0",
    "postcss-px-to-viewport": "^1.1.1",
    "sass": "^1.80.6",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "ts-loader": "^8.4.0",
    "typescript": "^4.9.5",
    "url-loader": "^1.0.1",
    "vue-devtools": "^5.1.4",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  }
}
